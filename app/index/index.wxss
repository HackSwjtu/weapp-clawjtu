.view_top_banner {
  display: flex;
  padding: 1rem;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid rgb(209, 209, 209);
  position: fixed;
  z-index: 100;
  top: 0px;
  background-color: rgba(255,255,255,0.96);
  width: 90%;
  height: 3rem;
}
.view_banner_elements {
  display: flex;
  justify-content: center;
}
.image_banner_avatar {
  width: 50px;
  height: 50px;
  border-radius: 50px;
  margin-right: 10px;
  flex-grow: 0;
  flex-shrink: 0;
  flex-basis: auto;
}
.text_banner {
  font-size: 15px;
  color: #a1a1a1;
  align-self: center;
}

.navigator_banner {
  display: flex;
  align-items: center;
  width: 40px;
  height: 40px;
  text-align: center;
  justify-content: center;
  border-width: 1px;
  border-style: solid;
  border-color: rgb(227, 227, 227);
  border-radius: 2px;
  position: relative;
  margin-left: 5px;
  margin-right: 5px;
}
.image_banner_button {
  width: 25px;
  height: 25px;
}